<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>cadastroClientesWeb</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>cardapioListar</servlet-name>
    <servlet-class>servlets.ListarCardapioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cardapioListar</servlet-name>
    <url-pattern>/listarCardapios</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>cadastroCardapio</servlet-name>
    <servlet-class>servlets.CadastroCardapioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cadastroCardapio</servlet-name>
    <url-pattern>/cadastroCardapio</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>RemoverCardapioServlet</servlet-name>
    <servlet-class>servlets.RemoverCardapioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RemoverCardapioServlet</servlet-name>
    <url-pattern>/removerCardapio</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/logar</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>